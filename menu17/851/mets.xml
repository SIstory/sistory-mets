            <dcterms:publisher>Državni zbor Republike Slovenije : Zveza zgodovinskih društev Slovenije, Ljubljana</dcterms:publisher>
            <dcterms:contributor>Jurij Perovšek (ur.)</dcterms:contributor>
            <dcterms:contributor>Cvetka Puncer (prevod)</dcterms:contributor>
            <dcterms:contributor>Barbara Šatej</dcterms:contributor>
            <dcterms:date xsi:type="dcterms:W3CDTF">2002</dcterms:date>
