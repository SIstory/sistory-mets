            <dcterms:subject xml:lang="eng">Slovenes in Argentina</dcterms:subject>
            <dcterms:subject xml:lang="eng">Slovenes in Australia</dcterms:subject>
            <dcterms:subject xml:lang="eng">painting</dcterms:subject>
            <dcterms:subject xml:lang="eng">landscape</dcterms:subject>
            <dcterms:subject xml:lang="eng">national symbolism</dcterms:subject>
