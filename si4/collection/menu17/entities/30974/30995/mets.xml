         <METS:xmlData>
            <dc:title xml:lang="slv">Nekaj spominskih utrinkov in razmišljanj ob 80-letnici dr. Štefana Trojarja</dc:title>
            <dc:creator>Šuligoj, Ljubica</dc:creator>
            <dc:subject xml:lang="slv">Štefan Trojar</dc:subject>
            <dc:subject xml:lang="slv">spomini</dc:subject>
