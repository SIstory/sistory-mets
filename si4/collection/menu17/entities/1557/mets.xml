            <dc:subject xml:lang="eng">communism</dc:subject>
            <dc:subject xml:lang="eng">Catholicism</dc:subject>
            <dc:subject xml:lang="eng">Dolomite Declaration</dc:subject>
            <dc:publisher>Inštitut za novejšo zgodovino, Ljubljana</dc:publisher>
