         <METS:xmlData>
            <dc:title xml:lang="slv">Rokovnjaški mit</dc:title>
            <dc:title xml:lang="eng">The Myth about the Brigands</dc:title>
            <dc:creator>Stariha, Gorazd</dc:creator>
            <dc:subject xml:lang="slv">rokovnjaštvo</dc:subject>
            <dc:subject xml:lang="slv">roparji</dc:subject>
