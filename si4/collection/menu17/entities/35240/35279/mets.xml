            <dc:contributor>Branka Petkovšek (ur.)</dc:contributor>
            <dc:contributor>Nives Sulič Dular (prev. ang.)</dc:contributor>
            <dc:contributor>Matjaž Klemenčič (prev. ang.)</dc:contributor>
            <dc:contributor>Stanislav Južnič (prev. ang.)</dc:contributor>
            <dc:date xsi:type="dcterms:W3CDTF">2006</dc:date>
