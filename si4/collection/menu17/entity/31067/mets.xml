            <dcterms:subject xml:lang="slv">travmatska stresna motnja</dcterms:subject>
            <dcterms:subject xml:lang="eng">Titoism, childhood</dcterms:subject>
            <dcterms:subject xml:lang="eng">orphans</dcterms:subject>
            <dcterms:subject xml:lang="eng">tabooed victims and trauma</dcterms:subject>
            <dcterms:subject xml:lang="eng">traumatic stress disorder</dcterms:subject>
