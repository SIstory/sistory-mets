            <dcterms:publisher>Inštitut za novejšo zgodovino, Ljubljana</dcterms:publisher>
            <dcterms:contributor>Damijan Guštin (moderator)</dcterms:contributor>
            <dcterms:date xsi:type="dcterms:W3CDTF">2014-10-23</dcterms:date>
