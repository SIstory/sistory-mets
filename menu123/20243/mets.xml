   <METS:dmdSec ID="dc.20243" GROUPID="20243">
      <METS:mdWrap MDTYPE="DC" MIMETYPE="text/xml">
         <METS:xmlData>
            <dcterms:title xml:lang="eng">15th International Symposium on School Life and School History Museums</dcterms:title>
            <dcterms:title xml:lang="eng">Creating links in education. Teacher’s and their associations as promoters of pedagogic development (historical and museum aspects)</dcterms:title>
            <dcterms:title xml:lang="eng">Ljubljana, 26 - 29th June 2013</dcterms:title>
            <dcterms:alternative xml:lang="slv">15. mednarodni simpozij šolskih muzejev in šolsko-zgodovinskih zbirk</dcterms:alternative>
            <dcterms:alternative xml:lang="slv">Povezovanje v izobraževanju: učiteljstvo in njihova društva kot spodbujevalci pedagoškega razvoja (zgodovinski in muzejski pogledi)</dcterms:alternative>
            <dcterms:alternative xml:lang="deu">/ 15. Internationales Symposium der Schulmuseen und Schulgeschichtlichen Sammlungen</dcterms:alternative>
            <dcterms:alternative xml:lang="deu">/ Vernetzung im Bereich Bildung. Der Einfluss von Lehrpersonen und deren Berufsorganisationen auf die Entwicklung der Schule und des Bildungswesens (historische und museologische Aspekte)</dcterms:alternative>
            <dcterms:alternative xml:lang="spa">15º Simposio Internacional de Museos y Colecciones de la Vida e Historia Escolar</dcterms:alternative>
            <dcterms:alternative xml:lang="spa">Conexiones en la Educación: Los Maestros y sus asociaciones como estímulos del Desarrollo Pedagógico (aspectos históricos y museológicos)</dcterms:alternative>
            <dcterms:subject xml:lang="slv">zgodovina</dcterms:subject>
            <dcterms:subject xml:lang="slv">šolstvo</dcterms:subject>
            <dcterms:subject xml:lang="slv">muzej</dcterms:subject>
            <dcterms:subject xml:lang="eng">history</dcterms:subject>
            <dcterms:subject xml:lang="eng">school system</dcterms:subject>
            <dcterms:subject xml:lang="eng">museum</dcterms:subject>
            <dcterms:publisher>Slovenski šolski muzej, Ljubljana</dcterms:publisher>
            <dcterms:publisher>Zveza zgodovinskih društev Slovenije, Ljubljana</dcterms:publisher>
            <dcterms:publisher>ICOM Slovenija, Celje</dcterms:publisher>
            <dcterms:publisher>Inštitut za novejšo zgodovino, Ljubljana</dcterms:publisher>
            <dcterms:date xsi:type="dcterms:W3CDTF">2013</dcterms:date>
            <dcterms:type xsi:type="dcterms:DCMIType">MovingImage</dcterms:type>
            <dcterms:language xsi:type="dcterms:ISO639-3">eng</dcterms:language>
