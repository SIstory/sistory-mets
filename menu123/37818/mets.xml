            <dcterms:subject xml:lang="slv">communism</dcterms:subject>
            <dcterms:subject xml:lang="slv">crimes</dcterms:subject>
            <dcterms:subject xml:lang="slv">traumas</dcterms:subject>
            <dcterms:subject xml:lang="slv">erasure of memory</dcterms:subject>
            <dcterms:publisher>Zveza zgodovinskih društev Slovenije, Ljubljana</dcterms:publisher>
            <dcterms:publisher>Inštitut za novejšo zgodovino, Ljubljana</dcterms:publisher>
